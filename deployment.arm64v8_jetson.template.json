{
  "$schema-template": "4.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "aiotdevopsacr": {
                "username": "${ACR_UNAME}",
                "password": "${ACR_PWD}",
                "address": "${ACR_ADDR}"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2.6-linux-arm64v8",
              "createOptions": {}
            },
            "env": {
              "storageFolder": {
                "value": "edge_agent_volume"
              }
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2.6-linux-arm64v8",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            },
            "env": {
              "storageFolder": {
                "value": "edge_hub_volume"
              }
            }
          }
        },
        "modules": {
          "Mfg_Vision_CIS_Camera_1": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_CIS_Camera_1}",
              "createOptions": {
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "HostConfig": {
                  "IpcMode": "host",
                  "gpus": 0,
                  "runtime": "nvidia",
                  "NetworkMode": "host",
                  "Binds": [
                    "/home/edge_assets/images_volume:/images_volume",
                    "/home/edge_assets/model_volume:/model_volume",
                    "/home/edge_assets/image_sink_volume/1:/image_sink_volume"
                  ],
                  "Mounts": [
                    {
                      "Type": "volume",
                      "Source": "images_volume",
                      "Target": "/var/opt/images_volume"
                    },
                    {
                      "Type": "volume",
                      "Source": "model_volume",
                      "Target": "/var/opt/model_volume"
                    }
                  ]
                }
              }
            }
          },
          "Mfg_Vision_Model_Repo": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_Model_Repo}",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "/home/edge_assets/model_volume:/model_volume"
                  ]
                },
                "Mounts": [
                  {
                    "Type": "volume",
                    "Source": "model_volume",
                    "Target": "/var/opt/model_volume"
                  }
                ]
              }
            }
          },
          "Mfg_Vision_Image_Upload": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_Image_Upload}",
              "createOptions": {
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "HostConfig": {
                  "NetworkMode": "host",
                  "Binds": [
                    "/home/edge_assets/images_volume:/images_volume"
                  ],
                  "Mounts": [
                    {
                      "Type": "volume",
                      "Source": "images_volume",
                      "Target": "/var/opt/images"
                    }
                  ]
                }
              }
            },
            "env": {
              "ADL_G2": {
                "value": "false"
              },
              "STORE_CONN_STR": {
                "value": "${STORAGE_CONN_STR}"
              }
            }
          },
          "Mfg_Vision_File_Cleanup": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_File_Cleanup}",
              "createOptions": {
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "HostConfig": {
                  "NetworkMode": "host",
                  "Binds": [
                    "/home/edge_assets/images_volume:/images_volume"
                  ],
                  "Mounts": [
                    {
                      "Type": "volume",
                      "Source": "images_volume",
                      "Target": "/var/opt/images_volume"
                    }
                  ]
                }
              }
            },
            "env": {
              "RETENTION_POLICY_SECONDS": {
                "value": "7200"
              }
            }
          },
          "Mfg_Vision_InfluxDB": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_InfluxDB}",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "/influxdata:/var/lib/influxdb"
                  ],
                  "PortBindings": {
                    "8086/tcp": [
                      {
                        "HostPort": "8086"
                      }
                    ]
                  }
                }
              }
            }
          },
          "Mfg_Vision_Dashboard_Grafana": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_Dashboard_Grafana}",
              "createOptions": {
                "Env": [
                  "GF_SECURITY_ADMIN_PASSWORD=Pa$$word321"
                ],
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "Volumes": {
                  "/etc/grafana/provisioning": {}
                },
                "HostConfig": {
                  "PortBindings": {
                    "3000/tcp": [
                      {
                        "HostPort": "3000"
                      }
                    ]
                  },
                  "NetworkMode": "host",
                  "Binds": [
                    "/home/edge_assets/images_volume:/usr/share/grafana/public/images"
                  ],
                  "Mounts": [
                    {
                      "Type": "volume",
                      "Source": "images_volume",
                      "Target": "/var/opt/images_volume"
                    }
                  ]
                }
              }
            },
            "env": {
              "GF_SERVER_PROTOCOL": {
                "value": "http"
              },
              "GF_DASHBOARDS_MIN_REFRESH_INTERVAL": {
                "value": "100ms"
              },
              "GF_DOMAIN": {
                "value": "localhost"
              }
            }
          },
          "Mfg_Vision_Dashboard_Custom": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.Mfg_Vision_Dashboard_Custom}",
              "createOptions": {
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "HostConfig": {
                  "PortBindings": {
                    "80/tcp": [
                      {
                        "HostPort": "22000"
                      }
                    ]
                  },
                  "IpcMode": "host",
                  "NetworkMode": "host",
                  "Binds": [
                    "/home/edge_assets/images_volume:/app/static/images"
                  ],
                  "Mounts": [
                    {
                      "Type": "volume",
                      "Source": "images_volume",
                      "Target": "/var/opt/images_volume"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "Mfg_Vision_CIS_Camera1_Inference_Upload": "FROM /messages/modules/Mfg_Vision_CIS_Camera1/outputs/outputInference INTO $upstream",
          "Mfg_Vision_CIS_Camera1_Image_Upload": "FROM /messages/modules/Mfg_Vision_CIS_Camera1/outputs/outputImageSend INTO BrokeredEndpoint(\"/modules/Mfg_Vision_Image_Upload/inputs/inputImageSend\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}